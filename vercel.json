{
  "version": 2,
  "builds": [
    {
      "src": "./server/dist/index.js",
      "use": "@vercel/bun"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/views/(.*)",
      "dest": "/views/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server/dist/index.js"
    }
  ]
}
